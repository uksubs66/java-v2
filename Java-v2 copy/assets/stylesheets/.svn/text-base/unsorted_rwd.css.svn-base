div.list_calendar {
    /*
    background-color:#efefef;
    border: 1px solid #aaa;
    line-height:22px;
    font-size:14px; 
    
    margin-bottom: 20px;
    margin-top: 20px;
    padding:10px;
    margin-bottom:20px;
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
    */

    margin: 20px auto 20px auto;

    width: 100%; /* Set width of table here, set height of cell below in td class */
}

#list_calendar .print_button {
    display: block;
    margin: 20px auto 20px auto;
    width: 120px;
    overflow:visible;
}

table.list_calendar {
    border-collapse: collapse;
    border: 0px;
    width:100%;
    table-layout: fixed;
    white-space: nowrap;
}

table.list_calendar caption {

    -webkit-border-radius: 3px 3px 0 0;
    -moz-border-radius: 3px 3px 0 0;
    border-radius: 3px 3px 0 0;
    background-color:#fafafa;
    line-height: 30px;
    border: 1px solid #aaa;
    border-bottom-width: 0px;

}

table.list_calendar thead {
    border: 1px solid #222; 
}

table.list_calendar thead tr {
    background-color:#8B8970;
    color:#000;
    font-size:100%;
    text-align:center;
    line-height:200%;
    /*padding:5px;*/
}

table.list_calendar thead th {
    border:1px solid #222;
    /*padding:2px;*/
}

table.list_calendar tbody {
    border:1px solid #CCCCAA;
}

/* Base 'td' definition */
table.list_calendar td {
    border:1px solid #CCCCAA;
    background-color: #ffffff;
    margin:0;
    padding:0;
    width:14.3%;
    text-align:left;
    vertical-align: top;
    color:#000;
    font-size: 80%;
    overflow: visible;
    height: 105px; /* Set height of cell here */
}

/* Old, out of date days */
table.list_calendar td.old {
    background-color: #F5F5DC;
}

table.list_calendar td .day {
    font-weight: bold;
    font-size: 110%;
    padding: 5px;
    margin: 2px;
    line-height: 100%;
    display: block;
    border: 1px solid #eaeaea;
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
}

table.list_calendar div.day {
    color: #999;
    border: 1px solid transparent;
}

table.list_calendar td.today a.day {
    background-color: lightblue;
}

/* Empty day slots */
table.list_calendar td.empty {
    background-color: #F5F5DC;
}

table.list_calendar .blocked1 {
    background-color:#CCCCAA;
    color:#FFFFFF;
}

table.list_calendar .grey {
    background-color:#e8e8e8;
    color:#000000;
    /* font-weight:bold; */
}

table.list_calendar td .item_container {
    display: block;
    white-space: nowrap;
    line-height: 18px;
    font-size: 10px;
    padding:0;
    margin:0;
    
}

table.list_calendar td .item_container a, 
table.list_calendar td .item_container div {
    text-decoration:none;
    padding:0 3px 0 3px; 
    display:block;
    overflow: hidden;
}

table.list_calendar td .item_container span.item_state {
    
    display: none;
    
}

table.list_calendar td div.day_wrapper .item_container a b {
    text-decoration: underline;
}

table.list_calendar td .item_container a:hover, 
table.list_calendar td a.day:hover {
    background-color:#333;
    color:#ffffff;

}

table.list_calendar td .item_container a.ical_button {
    padding:0;
    margin:0;
    float:right;
}

table.list_calendar .blocked1 a {
    color:#FFFFFF;

}

table.list_calendar td div.day_wrapper, table.list_calendar td div.day_wrapper_empty {
   height: 105px; /* Set height of cell here */
    /* height: 100%; */
    width: 100%; 
}

table.list_calendar td div.day_wrapper {

    position: relative;
    overflow: hidden;
    top: 0;
    left: 0;
    

}

table.list_calendar td div.day_wrapper.cal_expand {
    
    position: relative;
    overflow: visible;
    
}

table.list_calendar td div.day_wrapper div.day_container {

    top: 0;
    left: 0;
    overflow: hidden;
    border: none;
    -moz-box-shadow: none;
    -webkit-box-shadow: none;
    box-shadow: none;
}

table.list_calendar td div.day_wrapper div.day_container.cal_expand {
    position: absolute;
    background-color: #ffffff;
    border:1px solid #AAAA88;
    top: -1px;
    left: -1px;
    -moz-box-shadow: 2px 2px 3px #888;
    -webkit-box-shadow: 2px 2px 3px #888;
    box-shadow: 2px 2px 3px #888;
    border-collapse: separate; /* so ie9 renders box-shadow inside table cell */
    z-index: 100 !important;
}

table.list_calendar td div.day_wrapper .more_mask {

    position: absolute;
    top:0;
    left:0;
    bottom:0;
    right:0;
    background-image: url(../images/cal_more.png);
    background-position: bottom right;
    background-repeat: no-repeat;
     
}

table.list_calendar td div.day_wrapper .more_mask b {
    font-weight: normal;
    position: absolute;
    bottom:0;
    right:0;
    display:block;
    color: #ffffff;
    font-size: 9px;
    line-height:9px;
    margin-right: 2px;
    margin-bottom: 1px;
    
}

table.list_calendar td.find_overflow {
    width:auto;
    overflow:visible;
}

table.list_calendar td.find_overflow div.day_wrapper  {
    overflow: visible !important;
    display: inline-block !important;
    width:auto !important;
    height:auto !important;
    
}

table.list_calendar td.find_overflow div.day_wrapper .more_mask {
    /* display:none; */
}

table.list_calendar td.find_overflow div.day_wrapper div.day_container {
    
    overflow: visible !important;
    float:left !important; 
    
}

table.list_calendar td.find_overflow div.day_wrapper .item_container a.ical_button+a, 
table.list_calendar td.find_overflow div.day_wrapper .item_container a.ical_button+div {
    
    overflow: visible !important;
    display: inline-block !important;
    width:auto !important;
    
}

table.list_calendar td.find_overflow div.day_wrapper .item_container a.ical_button {
    float: none;
    display: inline-block !important;
    overflow: hidden;
    height: 5px;
}

