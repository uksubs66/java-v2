tr.partnerList td {
}

tr.partnerList ul {
    list-style-type: none;
}

tr.partnerList li  {
    list-style-type: none;
    padding: 0;
    margin: 0;
    height: 24px;
    vertical-align: middle;
    text-align: left;
    line-height: 24px;
    font-size: 13px;
    clear: both;
    white-space: nowrap;
}

tr.partnerList li.deleting {
   overflow: hidden;
}

tr.partnerList li div.sortItem  {
    position:relative;
}

tr.partnerList li.deleting div.sortItem {
   background-color:gold;
   opacity:0.4;
   filter:alpha(opacity=40); /* For IE8 and earlier */
}

tr.partnerList li span {
    display:inline;
}

tr.partnerList div.sortHandle{
    display: block;
    position: absolute;
    padding: 4px;
    padding-left: 15px;
    cursor:move;
    width: 16px;
    height: 16px;
    top:0px;
    right:0px;
}


.deleteButtonSmall{
    display:inline-block;
    width: 16px;
    height: 16px;
    background-image: url('../images/minus_circle.png');
    background-position: center center;
    background-repeat: no-repeat;
    padding:0;
    margin:0;
}

tr.partnerList a.deleteButtonSmall{
    float: left;
    display:block;
    width: 24px;
    height: 24px;
}

.deleteButtonSmall b{
    display:none;
    
}

#partnerlistiframediv table td {
    padding:0;
    margin:0;
    border:0;
    width: 50%;
}


/* ------- END SETTINGS PAGE ---------- */

.ajax_loader {
    -moz-border-radius: 4px;
    border-radius: 4px;
    border:#666666 1px solid;
    background-color: #ffffff;
    padding:5px;
}

.clearfloat{
    clear:both;
    visibility: hidden;
}
.clearfix:after {
    content: ".";
    display: block;
    line-height:0;
    height:0;
    clear:both;
    visibility:hidden;
}

/* Member announce tweaks */

div.announcement_container div.widget_list_primary {
    width: auto;
    float: none;
    margin: 0;
    overflow: hidden;
}
div.announcement_container div.widget_list_primary .widget_content {
    overflow:auto;
}
div.announcement_container div.widget_list_primary .weather_widget_accuweather_wide {
    overflow:hidden;
    display:none;
}
div.announcement_container div.widget_list_primary .widget_content:empty {
    display:none;
}


/* Help/information blocks */
.pageHelpHeader {
    display:none;
}