package com.foretees.api.clientReportTypes;

import java.util.*;

/**
 *
 * @author Owner
 */
public class activityReservationsBarChart {

    public final String type = this.getClass().getSimpleName();
    public String date;
    public String title;
    public Integer activity_id;
    public List<String> categories;

    public activityReservationsBarChart() {
    }

    public activityReservationsBarChart(String date, String title, Integer activity_id) {

        this.date = date;
        this.title = title;
        this.activity_id = activity_id;

    }

    public activityReservationsBarChart(String date, String title, Integer activity_id, List<String> categories) {

        this.date = date;
        this.title = title;
        this.activity_id = activity_id;
        this.categories = categories;

    }
}
